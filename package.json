{
  "name": "supplejs",
  "description": "SuppleJS is a toy project to re-implement SolidJS from scratch",
  "version": "0.8.1",
  "author": {
    "name": "delph123"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/delph123/supplejs.git"
  },
  "private": true,
  "type": "module",
  "prettier": {
    "editorconfig": true
