    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^8.46.0",
    "eslint-plugin-jest-dom": "^5.0.2",
    "eslint-plugin-testing-library": "^5.11.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.34.1",
    "vitest-dom": "^0.1.0"
  },
  "dependencies": {
    "rainbow-gradient": "^1.0.0"
