    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1"
  },
  "dependencies": {
    "rainbow-gradient": "^1.0.0"
