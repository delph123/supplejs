{
  "name": "supplejs",
  "description": "SuppleJS is a toy project to re-implement SolidJS from scratch",
  "version": "1.1.1",
  "author": {
    "name": "delph123"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/delph123/supplejs.git"
  },
  "main": "src/core/index.ts",
  "type": "module",
  "prettier": {
    "editorconfig": true
  },
  "publishConfig": {
    "directory": "build",
    "provenance": true
  },
  "clean-package": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.js",
    "exports": "./lib/index.js",
    "scripts": {},
    "dependencies": {},
    "devDependencies": {}
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && shx cp dist/index.html dist/404.html",
    "test": "vitest",
    "doc": "typedoc",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepack": "shx rm -rf build",
    "prepare": "tsc -p tsconfig.release.json && node scripts/prepare-release.js"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitest/coverage-v8": "^1.6.0",
    "csstype": "^3.1.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "shx": "^0.3.4",
    "typedoc": "^0.26.3",
    "typescript": "^5.5.2",
    "vite": "^5.3.2",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1"
  },
  "dependencies": {
    "rainbow-gradient": "^1.0.0"
  }
}
